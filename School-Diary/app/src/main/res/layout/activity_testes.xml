<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:agendaCalendar="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple_700"
    tools:context=".activity.Testes">


    <ImageView
        android:id="@+id/backbutton8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="10dp"
        android:elevation="100dp"
        agendaCalendar:layout_constraintStart_toStartOf="parent"
        agendaCalendar:layout_constraintTop_toTopOf="parent"
        agendaCalendar:srcCompat="@drawable/back" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:layout_marginTop="16dp"
        android:elevation="1dp"
        android:fontFamily="@font/brlnsr"
        android:text="Testes"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        agendaCalendar:layout_constraintEnd_toEndOf="parent"
        agendaCalendar:layout_constraintStart_toStartOf="parent"
        agendaCalendar:layout_constraintTop_toTopOf="parent" />

    <com.priyanka.customcalendarlibrary.AgendaCalendarView
        android:id="@+id/agenda_calendar_view"
        android:layout_width="393dp"
        android:layout_height="747dp"
        agendaCalendar:agendaCurrentDayTextColor="@color/theme_primary"
        agendaCalendar:calendarColor="@android:color/white"
        agendaCalendar:calendarCurrentDayTextColor="@color/calendar_text_current_day"
        agendaCalendar:calendarDayTextColor="@color/theme_primary"
        agendaCalendar:calendarHeaderColor="@color/theme_primary_dark"
        agendaCalendar:calendarPastDayTextColor="@android:color/darker_gray"
        agendaCalendar:fabColor="@color/theme_accent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" >

    </com.priyanka.customcalendarlibrary.AgendaCalendarView>

    <ImageView
        android:id="@+id/borda"
        android:layout_width="399dp"
        android:layout_height="25dp"
        android:layout_marginTop="71dp"
        android:elevation="20dp"
        agendaCalendar:layout_constraintEnd_toEndOf="parent"
        agendaCalendar:layout_constraintStart_toStartOf="parent"
        agendaCalendar:layout_constraintTop_toTopOf="parent"
        agendaCalendar:srcCompat="@drawable/algu" />


    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/add_btnhorario2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:clickable="true"
        android:focusable="true"
        android:tint="#FFFFFF"
        android:visibility="gone"
        agendaCalendar:backgroundTint="@color/roxoescuro"
        agendaCalendar:elevation="100dp"
        agendaCalendar:layout_constraintBottom_toTopOf="@+id/add_btn5"
        agendaCalendar:layout_constraintEnd_toEndOf="@+id/add_btn5"
        agendaCalendar:layout_constraintStart_toStartOf="@+id/add_btn5"
        agendaCalendar:srcCompat="@drawable/ic_add" />

    <com.priyanka.customcalendarlibrary.widgets.FloatingActionButton
        android:id="@+id/add_btn5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:clickable="true"
        android:focusable="true"
        agendaCalendar:backgroundTint="@color/roxoescuro"
        agendaCalendar:layout_constraintBottom_toBottomOf="parent"
        agendaCalendar:layout_constraintEnd_toEndOf="parent"
        agendaCalendar:layout_constraintHorizontal_bias="0.954"
        agendaCalendar:layout_constraintStart_toStartOf="parent"
        agendaCalendar:srcCompat="@drawable/ic_add" />


    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="399dp"
        android:layout_height="110dp"
        android:layout_marginBottom="744dp"
        agendaCalendar:layout_constraintBottom_toBottomOf="parent"
        agendaCalendar:layout_constraintEnd_toEndOf="parent"
        agendaCalendar:layout_constraintStart_toStartOf="parent"
        agendaCalendar:srcCompat="@drawable/titulocalendario" />

    <Button
        android:id="@+id/grades"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="65dp"
        android:background="@drawable/ic_baseline_emoji_events_24"
        agendaCalendar:layout_constraintEnd_toEndOf="@+id/textView15"
        agendaCalendar:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/avgbtn"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/ic_baseline_insert_chart_24"
        agendaCalendar:layout_constraintBottom_toBottomOf="@+id/grades"
        agendaCalendar:layout_constraintEnd_toEndOf="parent"
        agendaCalendar:layout_constraintStart_toEndOf="@+id/grades"
        agendaCalendar:layout_constraintTop_toTopOf="@+id/grades" />

</androidx.constraintlayout.widget.ConstraintLayout>